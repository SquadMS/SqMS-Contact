{
    "name": "squadms/contact",
    "description": "The SquadMS Contact Management module.",
    "keywords": ["laravel", "squad", "squadms", "contact"],
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Patrick Henninger",
            "email": "privat@skyraptor.eu",
            "homepage": "https://skyraptor.eu/"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^9.0",
        "illuminate/database": "^9.0",
        "nesbot/carbon": "^2.0",
        "squadms/foundation": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "mockery/mockery": "^1.0",
        "orchestra/testbench": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "SquadMS\\Contact\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SquadMS\\Contact\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "SquadMS\\Contact\\ContactServiceProvider"
            ]
        }
    },
    "config": {
        "allow-plugins": {
            "0.0.0/composer-include-files": true
        }
    }
}
